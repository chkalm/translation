{
    "name": "fork-299132/laravel-translation",
    "description": "A Translation package for Laravel 9 with database and cache support. Fork of the package Waavi Translation.",
    "keywords": [
        "laravel-translator",
        "laravel",
        "translator",
        "translation",
        "localization"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0",
        "doctrine/dbal": "^3.5"
    },
    "require-dev": {
        "phpunit/phpunit" : "^10.0",
        "orchestra/testbench": "^8.14",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Waavi\\Translation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Waavi\\Translation\\Test\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
